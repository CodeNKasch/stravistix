{
	"files": [
		"core.d.ts"
	],
	"include": [
		"**/*.ts"
	],
	"exclude": [
		"node_modules",
		"specs/**/*.ts"
	],
	"compilerOptions": {
		"outDir": "../../dist/core/",
		"target": "es5",
		"module": "commonjs",
		"lib": [
			"dom",
			"es6",
			"webworker"
		],
		"noImplicitAny": true,
		"preserveConstEnums": true,
		"inlineSourceMap": true,
		"inlineSources": true,
		"skipLibCheck": true
	},
	"compileOnSave": false
}
