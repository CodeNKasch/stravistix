{
	"files": [
		"core.d.ts"
	],
	"include": [
		"**/*.ts"
	],
	"exclude": [
		"node_modules",
		"specs/**/*.ts"
	],
	"compilerOptions": {
		"target": "es5",
		"module": "commonjs",
		"lib": [
			"dom",
			"es2017",
			"webworker"
		],
		"typeRoots": [
			"node_modules/@types"
		],
		"noImplicitAny": true,
		"preserveConstEnums": true,
		"skipLibCheck": true
	},
	"compileOnSave": false
}
